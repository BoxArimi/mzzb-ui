{
  "name": "react-scripts-ts",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:9090",
  "dependencies": {
    "antd": "^3.2.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "yarn build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-app-rewired start --scripts-version react-scripts-ts",
    "build-js": "react-app-rewired build --scripts-version react-scripts-ts",
    "start": "npm-run-all -p watch-css start-js",
    "build": "yarn build-css && yarn build-js",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/antd": "^1.0.0",
    "@types/jest": "^22.1.2",
    "@types/node": "^9.4.5",
    "@types/react": "^16.0.36",
    "@types/react-dom": "^16.0.3",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.2",
    "react-app-rewired": "^1.4.1",
    "react-scripts-ts": "2.13.0",
    "ts-import-plugin": "^1.4.3",
    "typescript": "^2.7.1"
  }
}
