{
  "private": true,
  "name": "mzzb-ui",
  "version": "0.7.0",
  "license": "Apache-2.0",
  "description": "ui for mingzuozhibi",
  "dependencies": {
    "antd": "^3.2.1",
    "immer": "^1.0.1",
    "lodash.debounce": "^4.0.8",
    "md5": "^2.2.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-loadable": "^5.3.1",
    "react-router-dom": "^4.2.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "yarn build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-ts": "react-app-rewired start --scripts-version react-scripts-ts",
    "build-ts": "react-app-rewired build --scripts-version react-scripts-ts",
    "start": "npm-run-all -p watch-css start-ts",
    "build": "yarn build-css && yarn build-ts",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/antd": "^1.0.0",
    "@types/jest": "^22.1.2",
    "@types/lodash.debounce": "^4.0.3",
    "@types/md5": "^2.1.32",
    "@types/node": "^9.4.5",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.36",
    "@types/react-dom": "^16.0.3",
    "@types/react-loadable": "^5.3.3",
    "@types/react-router-dom": "^4.2.4",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.2",
    "react-app-rewired": "^1.4.1",
    "react-scripts-ts": "2.13.0",
    "ts-import-plugin": "^1.4.3",
    "typescript": "^2.7.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mingzuozhibi/mzzb-ui.git"
